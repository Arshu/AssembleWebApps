{
  "WebRootName": "wwwroot",
  "ConfigType": "Base",
  "ConfigLogLevel": "Critical,Error,Warn,Debug",
  "CreateWebConfig": false,
  "CreateApiConfig": false,
  "SaveJsCssResource": false,
  "PersistMetricLog": false,
  "AllowReload": true,
  "AllowConfigUpdate": false,

  "ContextJson": {
    "PageTitle": "Arshu AppWeb Viewer",
    "ShowIntroTip": false,
    "DefaultRecordMetrics": true
  },

  "CacheWebCompression": "br,gzip,deflate",
  "NormalWebCompression": "br,gzip,deflate",
  "MetricPort": 0,
  "InitialTimeInSec": 0,
  "IdleTimeInSec": 0,
  "WSClientHeartBeatTimeInMs": 60000,
  "WSServerHeartBeatTimeInMs": 60000,
  "WSServerList": [
    {
      "ServerUrl": "https://appwebdata.fly.dev",
      "ClusterName": "appwebcluster",
      "MethodPrefixes": "Save, Add, Delete, Edit, Clone, Get",
      "Enabled": false
    }
  ],

  "RealtimeAppSites": "",
  "StaticAppSites": "Index, Config, Account, Test",
  "SearchAppSites": "Config, Account, Index",
  "SearchAppFolders": "SampleJson",

  "LoadAppData": true,
  "LoadAppFolders": false,
  "LoadPapFiles": false,
  "LoadFileChanges": false,
  "UnPackAppSites": false,

  "AppAssets": [

    {
      "AssetTypeName": "Arshu.Core.AssetRegister",
      "AssetDll": "Arshu.Core.dll",
      "IsEnabled": true
    },
    {
      "AssetTypeName": "Arshu.AppBaseWeb.AssetRegister",
      "AssetDll": "Arshu.AppBaseWeb.dll",
      "IsEnabled": true
    }
  ],

  "AppServices": [

    {
      "RegisterTypeName": "Arshu.Core.App.Register.WebRegister",
      "RegisterDll": "Arshu.Core.dll",
      "IsEnabled": true
    },
    {
      "RegisterTypeName": "Arshu.Core.App.Register.VueRegister",
      "RegisterDll": "Arshu.Core.dll",
      "IsEnabled": true
    },
    {
      "RegisterTypeName": "Arshu.Core.App.Register.SyncRegister",
      "RegisterDll": "Arshu.Core.dll",
      "IsEnabled": true
    },
    {
      "RegisterTypeName": "Arshu.Git.App.Register.GitRegister",
      "RegisterDll": "Arshu.Git.dll",
      "IsEnabled": true
    }
  ],

  "AppProviders": [

    {
      "RegisterTypeName": "Arshu.Data.Register.DataRegister",
      "RegisterDll": "Arshu.Data.dll",
      "IsEnabled": false,
      "ProviderTypeName": "Microsoft.Data.Sqlite.SqliteFactory",
      "ProviderDll": "Microsoft.Data.Sqlite.dll"
    }
  ],

  "PackWebRootName": "wwwroot",
  "SkipSystemDirectories": "_Meta, .git, Log, bin, obj, _bin_deployableAssemblies, Bak, Backup, Repository, Cache, Inbox, InboxLocal, InboxRemote",
  "SystemDirectories": "App_Config, App_Data, Db, Backup, Output, UndoRedo, Cache",
  "AllowDirectories": "",
  "SkipAppDirectories": "Arshu, Templates, Resource, Content, App_Config, App_Data, Backup, Output, UndoRedo, Cache",
  "SkipResourceDirectories": "Arshu, Component, AppSites, Templates, App_Config, App_Data, Backup, Output, UndoRedo, AppApi, Dynamic, Meta, Upload, Cache",
  "SkipDataDirectories": "Arshu, AppSites, Component, Templates, Resource, Content, Backup, Output, UndoRedo, Cache",

  "PackAppDirectories": "AppSites, AppApi",
  "PackResourcesDirectories": "Resource,Content",
  "PackDataDirectories": "App_Data, Db, AppApi",
  "PackAppWebDirectories": "SampleJson",
  "PackTemplateDirectories": "Templates",

  "PackApp": false,
  "PackResource": false,
  "PackData": true,
  "PackAppFolder": true,
  "PackTemplate": false,
  "PackAppSites": "Index, Account, Config, Test, Template, Fly",
  "PackAppDatas": "Roles, Members",

  "PackName": "Arshu,AppBaseWeb.1.0.pap",
  "PackDescription": "Arshu,AppBaseWeb for Managing AppWebs",
  "PackResourceName": "Arshu,AppBaseWeb.Resource.1.0.pap",
  "PackResourceDescription": "Resource of Arshu,AppBaseWeb for Managing AppWebs",
  "PackDataName": "Arshu,AppBaseWeb.Data.1.0.pap",
  "PackDataDescription": "Data of Arshu,AppBaseWeb for Managing AppWebs",
  "PackAppWebFolderName": "Arshu,AppBaseWeb.AppWebFolder.1.0.pap",
  "PackAppWebFolderDescription": "WebFolder of Arshu,AppBaseWeb for Managing AppWebs",
  "PackTemplateName": "Arshu,AppBaseWeb.Template.1.0.pap",
  "PackTemplateDescription": "Templates of Arshu,AppBaseWeb for Managing AppWebs",

  "PackAppSubFolderPath": "A0",
  "PackResourceSubFolderPath": "A0",
  "PackDataSubFolderPath": "A0",
  "PackTemplateSubFolderPath": "A0",

  "SkippedFileExtension": "dll, config, xml, exe, pap, so, map, txt",
  "AllowedAppFileExtension": "Html, Json",
  "AllowedResourceFileExtension": "Bmp, Png, Jpg, Jpeg, Gif, Ico, woff, woff2, otf, eot, svg, ttf, Js, Css, json",
  "AllowedDataFileExtension": "json, dat, xlsx, db, sdf",
  "AllowedTemplateFileExtension": "Html, Json, md, Bmp, Png, Jpg, Jpeg, Gif, Ico, woff, woff2, otf, eot, svg, ttf, Js, Css",

  "ResourceOptimizeMergeJs": true,
  "ResourceOptimizeMergeJsPattern": "base, arshu, main, highlight, chart; *",
  "ResourceOptimizeMergeJsSkipPattern": "",
  "ResourceOptimizeMergeCss": true,
  "ResourceOptimizeMergeCssPattern": "base, arshu, normalize, animate, main, highlight, chart; *",
  "ResourceOptimizeMergeCssSkipPattern": ""
}